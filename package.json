{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "bundle.js",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start:dev": "nodemon --env-file=.env server.js",
    "import-data": "nodemon --env-file=.env ./dev-data/script.js",
    "start:prod": "cross-env NODE_ENV=production nodemon --env-file=.env server.js",
    "debug": "ndb --env-file=.env server.js",
    "watch:js": "parcel ./public/js/index.js --dist-dir ./public/bundle ",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Abu Zain",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.0",
    "pub": "^0.2.0",
    "pug": "^3.0.2",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "stripe": "^15.8.0",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "parcel": "^2.12.0",
    "prettier": "^1.17.0"
  }
}
